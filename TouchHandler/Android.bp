android_app {
    name: "TouchHandler",
    defaults: ["SettingsLibDefaults"],

    srcs: ["src/**/*.java"],
    resource_dirs: ["res"],

    certificate: "platform",
    platform_apis: true,
    system_ext_specific: true,

    optimize: {
        proguard_flags_files: ["proguard.flags"],
    },

    static_libs: [
        "vendor.lineage.touch-V1.0-java",
        "VendorSupportLib",
    ],
}
